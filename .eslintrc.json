{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
	"airbnb",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
	"react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
	"import/no-extraneous-dependencies": ["error",  {"devDependencies": true}],
    	"react/jsx-props-no-spreading": "off",
	"react/prop-types": 0,
	"react/destructuring-assignment": "off"
    }
}
